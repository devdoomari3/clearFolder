{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "typeRoots": [
    "./custom_typings",
      "./node_modules/@types"
    ],
    "lib": [
      "es2015"
    ],
    "declaration": true,
    "jsx": "react",
    "rootDir": "src",
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "lib"
  },
  "exclude": [
    "node_modules",
    "__tests__",
    "lib",
    "tscompiled"

  ]
}